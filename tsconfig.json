{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "./node_modules/**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "paths": {
      "@/public/*": ["public/*"],
      "@/src/*": ["src/*"],
      "@/components/*": ["src/compontents/*"],
      "@/pages/*": ["src/pages/*"],
      "@/domain/*": ["src/domain/*"],
      "@/util/*": ["src/util/*"]
    }
  }
}
